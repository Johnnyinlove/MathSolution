Sub Guardar()




Dim Celdae As Range




If Hoja6.Range("M3").Value = "" Then

MsgBox "Entre el mes"

Else


If Hoja6.Range("M3").Value = 0 Then


MsgBox "El Mes tiene que ser un numero entre 01 y 12"

Else

If Hoja6.Range("M3").Value < 13 Then



If Hoja6.Range("M3").Value = Hoja7.Range("A2") Then

If Hoja6.Range("K3").Value = "" Then

MsgBox "Entre el dia"

Else

If Hoja6.Range("K3").Value = 0 Then

MsgBox "El Dia tiene que ser un numero entre 01 y 31"

Else

If Hoja6.Range("K3").Value < 32 Then




If Hoja6.Range("K3").Value = Hoja7.Range("V1").Value Then


Hoja7.Range("V4:AA18").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V20").Value Then


Hoja7.Range("V23:AA37").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V39").Value Then


Hoja7.Range("V42:AA56").Value = Hoja6.Range("K6:P20").Value


End If



If Hoja6.Range("K3").Value = Hoja7.Range("V58").Value Then


Hoja7.Range("V61:AA75").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V77").Value Then


Hoja7.Range("V80:AA94").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V96").Value Then


Hoja7.Range("V99:AA113").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V115").Value Then


Hoja7.Range("V118:AA132").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V134").Value Then


Hoja7.Range("V137:AA151").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V153").Value Then


Hoja7.Range("V156:AA170").Value = Hoja6.Range("K6:P20").Value


End If

If Hoja6.Range("K3").Value = Hoja7.Range("V172").Value Then


Hoja7.Range("V175:AA189").Value = Hoja6.Range("K6:P20").Value


End If

If Hoja6.Range("K3").Value = Hoja7.Range("V191").Value Then


Hoja7.Range("V194:AA208").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V210").Value Then


