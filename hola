End If



If Hoja6.Range("K3").Value = Hoja7.Range("V267").Value Then


Hoja7.Range("V270:AA284").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V286").Value Then


Hoja7.Range("V289:AA303").Value = Hoja6.Range("K6:P20").Value


End If

If Hoja6.Range("K3").Value = Hoja7.Range("V305").Value Then


