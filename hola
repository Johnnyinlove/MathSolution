End If


If Hoja6.Range("K3").Value = Hoja7.Range("V324").Value Then


Hoja7.Range("V327:AA341").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V343").Value Then


Hoja7.Range("V346:AA360").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V362").Value Then


Hoja7.Range("V365:AA379").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V381").Value Then


Hoja7.Range("V384:AA398").Value = Hoja6.Range("K6:P20").Value


End If


If Hoja6.Range("K3").Value = Hoja7.Range("V400").Value Then

Hoja7.Range("V403:AA417").Value = Hoja6.Range("K6:P20").Value

End If

If Hoja6.Range("K3").Value = Hoja7.Range("V419").Value Then

Hoja7.Range("V422:AA436").Value = Hoja6.Range("K6:P20").Value

End If


If Hoja6.Range("K3").Value = Hoja7.Range("V438").Value Then

Hoja7.Range("V441:AA455").Value = Hoja6.Range("K6:P20").Value

End If

If Hoja6.Range("K3").Value = Hoja7.Range("V457").Value Then

Hoja7.Range("V460:AA474").Value = Hoja6.Range("K6:P20").Value

End If


If Hoja6.Range("K3").Value = Hoja7.Range("V476").Value Then

Hoja7.Range("V479:AA493").Value = Hoja6.Range("K6:P20").Value

End If

If Hoja6.Range("K3").Value = Hoja7.Range("V495").Value Then

Hoja7.Range("V498:AA512").Value = Hoja6.Range("K6:P20").Value

End If

If Hoja6.Range("K3").Value = Hoja7.Range("V514").Value Then

Hoja7.Range("V517:AA531").Value = Hoja6.Range("K6:P20").Value

End If


If Hoja6.Range("K3").Value = Hoja7.Range("V533").Value Then

Hoja7.Range("V536:AA550").Value = Hoja6.Range("K6:P20").Value

End If

If Hoja6.Range("K3").Value = Hoja7.Range("V552").Value Then

Hoja7.Range("V555:AA569").Value = Hoja6.Range("K6:P20").Value

End If

If Hoja6.Range("K3").Value = Hoja7.Range("V571").Value Then

Hoja7.Range("V574:AA588").Value = Hoja6.Range("K6:P20").Value

End If




Else

MsgBox "El Dia tiene que ser un numero entre 01 y 31"


End If


End If


End If


End If






Hoja6.Range("K3").Value = ""
Hoja6.Range("M3").Value = ""
Hoja6.Range("K6:K20").Value = ""
Hoja6.Range("M6:M20").Value = ""
Hoja6.Range("O6:O20").Value = ""

End Sub









