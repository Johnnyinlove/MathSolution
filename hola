End If



Else

MsgBox "El Dia tiene que ser un numero entre 01 y 31"


End If


End If


End If


End If


Else

MsgBox "El Mes tiene que ser un numero entre 01 y 12"


End If

End If


End If




End Sub



Sub Nuevo()

Hoja6.Range("K3").Value = ""
Hoja6.Range("M3").Value = ""
Hoja6.Range("K6:K20").Value = ""
Hoja6.Range("M6:M20").Value = ""
Hoja6.Range("O6:O20").Value = ""

End Sub




